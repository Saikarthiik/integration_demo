			smartUtil.closeTab
			#Import the Excel file in to Application
			excelFileInteractions.importExcelIntoApplication universal data.supplierName
			#checking  for the Duplicate Record POP-UP
			if ImageFound(text:"Duplicates Records",wait :3)
				LogSuccess "Duplicated Records are Found in the Application"
				if step is "Step1" then
					smartUtil.clickTextAtSpot"NON-TAA",-412,10,"VM"
				end if 
				smartUtil.clickText"Continue","BH"
				if ImageFound(text:"Save",searchRectangle:(screenPartition().SUT.("THTH")))
					LogSuccess "Save Button is enabled in the Application"
					smartUtil.clickText"Save","THTH"
				else
					LogError "Save Button is Not Enabled in the Application Unable to Adjust te value in the Application"
				end if
			else
				LogError "There are no Duplicated Records Found in the Application"
			end if
			if ImageFound(text:"Successfully Created New Adjustment",searchRectangle:(screenPartition().SUT.("THTH")))
				LogSuccess "New Escalation is Raised Successfully"
			else 
				LogWarning "New Escalation is not created Successfully"
			end if
			#Checking the Adj value in Forecast View
			"SC2_CommonActions/sc2CommonHandlers".forecastPHVAdjValueVerification universal partNumber,universal ProdWeek,universal ProdWeek,universal data.NONTAAAdj
			#Checking the Adj value in KR Summary View
			"SC2_CommonActions/sc2CommonHandlers".krSummaryAdjvalueVerification universal partNumber,universal prodWeek,universal prodWeek,"NON-TAA",universal data.NONTAAAdj
			#Checking the Adj value in IRP Summary View
			"SC2_CommonActions/sc2CommonHandlers".irpSummaryAdjValueVerification universal partNumber,universal prodWeek,universal prodWeek,"NON-TAA",universal data.NONTAAAdj
		end repeat
	Catch theException
		smartUtil.errorCapture theException
	End try	
end massUploadPHViewAddNewWWPart2
