to observeFeatureChangeInForecastHorizontalView
	try
		#populate variables with test data from excel
		set supplierName to universal testData.supplierName
		set demandCategory to universal testData.demandCategory
		set partNumber to universal testData.partNumber
		set chosenWeek to universal testData.week
		set chosenYear to universal testData.year
		
		#validate and click on forecast and commit
		smartUtil.clicktext " Forecast & Commit","TH"
		smartUtil.setCustomSearchRectangle "Forecast & commit", -50,10,200,300
		#validate and click on Planner Horizontal View
		smartUtil.validate "Planner Horizontal View","Custompart"
		smartUtil.clickText "Planner Horizontal View","Custompart"
		WaitFor 30, text:"Planner Full-View"
		smartUtil.readTextUsingText "Week",70,-10,40,25,"THTH"
		--log global currLocVal
		delete  <nondigit> from global currLocVal
		put global currLocVal into  universal currentWeekValue
		log universal currentWeekValue
		"SC2_CommonActions/sc2CommonHandlers".navigateToSupplierName supplierName
		wait 10
		typetext pageDown
		smartUtil.validate "Best Practice", "BH"
		typetext pageUp
		wait 2
		smartUtil.setTextstyle "GrayText"
		smartUtil.clickText "Search","topLeftQuadrant"
		#entering part number and click on search icon
		smartUtil.textField partNumber
		smartUtil.clickUsingImage "icons/ForeCastAndCommitSearchIcon","topLeftQuadrant"
		wait 5
		#search for the icon
		smartUtil.setCustomSearchRectangle demandCategory, -260, -20, 50, 25, "BH"
		set the SearchRectangle to global SearchRect
		wait 5
		if ImageFound("icons/materialIconOfItem") then
			MoveTo FoundImageLocation()
			smartUtil.clickUsingImage "icons/materialIconOfItem","Custompart"
			wait 4
			smartUtil.setCustomSearchRectangle "Switch to SO View",-140,-60, 300, 360
			--	set the SearchRectangle to global SearchRect
			SET THE TextStyle TO {dpi:144}
			smartUtil.validate "Switch to SO View","Custompart"
			put chosenWeek & "/" & chosenYear into weekYearText
			smartUtil.validate "Week/Year", "Custompart"
			smartUtil.clickText"Week/Year", "Custompart"
			smartUtil.clickText universal currentWeekValue,"Custompart"
			smartUtil.clickText "OK","Custompart"
			SET THE TextStyle TO {dpi:72}
		else
			log "Error: Icon 'Navigate to SO View' is not found."
		end if
	Catch theException
		smartUtil.errorCapture theException
	End try	
end observeFeatureChangeInForecastHorizontalView

to UIValidationInPlannerAdjHorizontalView SupplierName
	try
		#Navigating to PlannerHorizontalView page
		"SC2_CommonActions/sc2CommonHandlers".navigateToPlannerHorizontalView
		smartUtil.completeScreenLoad
		#clicking on the Supplier Name
		"SC2_CommonActions/sc2CommonHandlers".navigateToSupplierName SupplierName
		#Validating that whether we are in the PlannerHorizontalView or not with some texts in the PlannerHorizontalView page 
		if ImageFound(text:"Planner Full-View",searchRectangle:(screenPartition().SUT.("THTH")))
			log "We are in the Horizontal View continue with Horizontal view validations"
			smartUtil.validate"Planner Full-View","THTH"
			smartUtil.validate"View TAA&NON-TAA","THTH"
			smartUtil.validate"Hide SYSTEM/HARDBOOK","THTH"
			smartUtil.clickText"Supplier","LHLH"
			wait 1
			smartUtil.validate"Filters","LHLH"
			#Reading the Current week from the PlannerHorizontalView page for validation
			Repeat until ImageFound(text:"Week From",searchRectangle:(screenPartition().SUT.("LHLH")))
				ScrollWheelDown  7
				smartUtil.validate"Week From"
			End repeat
			smartUtil.setCustomSearchRectangle "Week To",300,0,1000,80
			if ImageFound(image:"icons/unselectWeek",searchRectangle:(screenPartition().SUT.("Custompart"))) then
				smartUtil.clickUsingImage "icons/unselectWeek","Custompart"
			end if 
			smartUtil.clickText"Submit","LHLH"
			smartUtil.completeScreenLoad
			smartUtil.validate"Item No","LHLH"
			smartUtil.readTextUsingText "Week",70,-10,40,25,"THTH"
			delete  <nondigit> from global currLocVal
			put global currLocVal into  universal currentWeekValue
			log universal currentWeekValue
			put  universal currentWeekValue+7 into universal totalminimumweeksofHorizontalView
			log  universal totalminimumweeksofHorizontalView
			if universal totalminimumweeksofHorizontalView>52
				put universal totalminimumweeksofHorizontalView-52 into universal totalminimumweeksofHorizontalView
				log universal totalminimumweeksofHorizontalView
			else 
				put  universal totalminimumweeksofHorizontalView into universal totalminimumweeksofHorizontalView
				log universal totalminimumweeksofHorizontalView
			end if
			Repeat until ImageFound(text:"TOTAL",waitFor:0.5,searchRectangle:(screenPartition().SUT.("TH")))
				Repeat 5 times 
					smartUtil.clickUsingImage "icons/rightScroll"
				end Repeat
			End Repeat
			smartUtil.readTextUsingText "TOTAL",-400,-50,100,25,"HM"
			log global currLocVal 
			if global currLocVal  contains universal totalminimumweeksofHorizontalView
				LogSuccess "Minium number of work Weeks are Selected and maximum Work weeks are"&&universal totalminimumweeksofHorizontalView
			else 
				LogError"Application is Not Filtered with minium number of Work week of"&&universal totalminimumweeksofHorizontalView
			end if
			put universal currentWeekValue+19 into universal totalmaximumweeksofHorizontalView
			log universal totalmaximumweeksofHorizontalView
			smartUtil.clickText"Supplier","LHLH"
			wait 1
			smartUtil.validate"Filters","LHLH"
			Repeat until ImageFound(text:"Week From",searchRectangle:(screenPartition().SUT.("LHLH")))
				ScrollWheelDown  7
				smartUtil.validate"Week From"
			End repeat
			smartUtil.clickTextAtSpot "Week From",260,25,"LH"
			"SC2_CommonActions/sc2CommonHandlers".selectWeekInKRSummary universal currentWeekValue
			##Adding more than 20 weeks in filter
			smartUtil.clickTextAtSpot "Week To",260,25,"LH"
			put universal currentWeekValue+22 into universal morethan20ww
			log universal morethan20ww
			if universal morethan20ww>52
				put universal morethan20ww-52-1 into universal wwinCalender
				log universal wwinCalender
			else
				put universal morethan20ww into universal wwinCalender
			end if 
			put universal currentWeekValue+19 into  universal ww20
			--log universal ww20
			if universal ww20>52
				put universal ww20-52 into  universal ww20inApplication
				log universal ww20inApplication
			else 
				put  universal ww20 into universal ww20inApplication
				log universal ww20inApplication
			end if
			"SC2_CommonActions/sc2CommonHandlers".selectWeekInKRSummary universal wwinCalender
			#click on the submit button
			smartUtil.clickText "Submit","LH"
			Repeat until ImageFound(text:"TOTAL",waitFor:0.5,searchRectangle:(screenPartition().SUT.("TH")))
				Repeat 11 times 
					smartUtil.clickUsingImage "icons/rightScroll"
				end Repeat
			End Repeat
			smartUtil.readTextUsingText "TOTAL",-400,-50,100,25,"TH"
			log global currLocVal
			if global currLocVal contains universal ww20inApplication
				LogSuccess"Maximum number of work Weeks are Selected and maximum Work weeks are"&&universal ww20inApplication
			else 
				LogError"Application is Not Filtered with maximum number of Work Week of"&&universal ww20inApplication
			end if
			#validating the total  maximum number of the weeks present in the UI
			smartUtil.validate"Planner Full-View","THTH"
			smartUtil.clickText "Planner Full-View","THTH"
			smartUtil.completeScreenLoad
			smartUtil.validate"Buffer","HM"
			smartUtil.validate"Buffer Opt","HM"
		else
			log Error:"We are not in the HorizontalView"
		end if
		set  global currLocVal to empty
	Catch theException
		smartUtil.errorCapture theException
	End try	
	
end UIValidationInPlannerAdjHorizontalView


to AddNewWorkWeekPlannerHorizontalView SupplierName,partNumber
	
	try
		"SC2_CommonActions/sc2CommonHandlers".navigateToPlannerHorizontalView
		smartUtil.completeScreenLoad
		smartUtil.readTextUsingText "Week",70,-10,40,25,"THTH"
		log global currLocVal
		delete  <nondigit> from global currLocVal
		put global currLocVal into  universal currentWW
		log universal currentWW
		put universal currentWW+11 into  universal defultWWOfKRS
		log universal defultWWOfKRS
		"SC2_CommonActions/sc2CommonHandlers".navigateToSupplierName SupplierName
		smartUtil.clickUsingImage"icons/AddNewOriPartNumber","topRightQuadrant"
		wait 3
		"review&Commit_F&C/ForeCast/foreCast_utilities".addNewPartAndWorkWeekInUI universal data.partNumber,universal data.categoryInForecast,universal data.PlannerRemarks,universal data.AdjustmentPositiveValue,universal data.EscalationReason
		smartUtil.clickText"Save","THTH"
		log universal ProdWeek
		split universal ProdWeek by "/"
		put  item 1 of universal ProdWeek into universal week
		log universal week
		--smartUtil.validate"Succuessfully Created New Adjestment","THTH"
		"SC2_CommonActions/sc2CommonHandlers".fandCHorizontalViewAdjvalueVerification universal data.category 
		--put  universal AdjValue into fandcAdjvalue
		Assert that universal fandcAdjvalue is universal data.AdjustmentPositiveValue
		if ( universal week<=universal defultWWOfKRS)
			"SC2_CommonActions/sc2CommonHandlers".krSummaryAdjvalueVerification universal data.partNumber,universal week,universal week, universal data.category,universal data.AdjustmentPositiveValue
		else 
			log "The KR Summary will have the data of 12 Weeks"
		end if
	Catch theException
		smartUtil.errorCapture theException
	End try	
	
end AddNewWorkWeekPlannerHorizontalView

to  UIValidationForAdjPlannerHorizontalViewPart1 SupplierName,partNumber,adjustmentPositiveValue,EscalationReason
	try
		#Step -1---------------
		#Navigating to Planner Horizontal View
		"SC2_CommonActions/sc2CommonHandlers".navigateToPlannerHorizontalView
		smartUtil.completeScreenLoad
		#Clicking on the Supplier Name
		"SC2_CommonActions/sc2CommonHandlers".navigateToSupplierName SupplierName
		smartUtil.completeScreenLoad
		#Searching for the Part Number
		smartUtil.clickText"Search","LHLH"
		smartUtil.textField universal data.partNumber
		log  universal data.partNumber
		smartUtil.clickUsingImage"icons/ForeCastAndCommitSearchIcon","LHLH"
		wait 5
		#Storing the Total KR  value  
		Repeat until ImageFound(text:"TOTAL",searchRectangle:(screenPartition().SUT.("RH")),waitFor:1)
			Repeat 5 times
				smartUtil.clickUsingImage"icons/rightScroll","bottomRightQuadrant"
			end repeat
			if repeatIndex() > 20
				log "TOTAL text is not found in the UI"
				exit repeat
			end if
		End repeat
		smartUtil.validate"TOTAL","rightHalfOfRightHalf"
		smartUtil.setCustomSearchRectangle"TOTAL",-90,20,250,300
		wait 0.9
		SET THE TextStyle TO {contrast:"on"}
		smartUtil.readTextUsingText "KR",-5,30,40,60,"Custompart"
		put  global currLocVal into universal KRValue
		log universal KRValue
		SET THE TextStyle TO {contrast:"off"}
		#Click on the Refresh Icon
		smartUtil.clickUsingImage  "icons/SCCRefreshIcon" ,"topRightQuadrant"
		wait 5
		#Entering the KR Value
		smartUtil.validate"KR","LHLH"
		smartUtil.clickTextAtSpot"KR",0,60,"LHLH"
		Typetext controlKey ,a,c
		put RemoteClipboard() into universal currentAdjvalue
		log  universal currentAdjvalue
		put universal currentAdjvalue+universal data.adjustmentPositiveValue into universal adjvalue
		smartUtil.textField universal adjvalue
		smartUtil.clickText"Forecast Phase","topRightQuadrant"
		if ImageFound(text:"Adj Reason",searchRectangle:(screenPartition().SUT.("LH")),waitFor:2)
			smartUtil.setCustomSearchRectangle"Adj Reason",-80,0,150,90
			smartUtil.clickUsingImage "icons/adjReasonInputDropdown","Custompart"
			smartUtil.clickText universal data.EscalationReason
			smartUtil.clickUsingImage "icons/saveIconInForecastAndCommit","THTH"
			smartUtil.validate"Save Successfully","THTH"
		else
			smartUtil.clickUsingImage "icons/saveIconInForecastAndCommit","THTH"
			smartUtil.validate"Save Successfully","THTH"
		end if	
		Repeat until ImageFound(text:"TOTAL",searchRectangle:(screenPartition().SUT.("RH")),waitFor:2)
			Repeat 5 times
				smartUtil.clickUsingImage"icons/rightScroll","bottomRightQuadrant"
			end repeat
			if repeatIndex() > 20
				log "TOTAL text is not found in the UI"
				exit repeat
			end if
		End repeat
		smartUtil.validate"TOTAL","rightHalfOfRightHalf"
		smartUtil.setCustomSearchRectangle"TOTAL",-90,20,250,300
		wait 7
		set the TextStyle to {dpi:144}
		smartUtil.readTextUsingText "KR",-5,30,40,70,"Custompart",LowResolutionMode:"Yes"
		put  global currLocVal into  universal updatedKRvalue
		put universal data.adjustmentPositiveValue+universal KRValue into universal updatedTotalValue
		if(universal updatedKRvalue = universal updatedTotalValue)
			LogSuccess"Adj value is Successfully Updated"
		else
			LogError "Adj value is not Updated Sucessfully"
		end if
		set the TextStyle to default
		#Reading the Current Week 
		smartUtil.readTextUsingText"Week",59,-10,38,15,"THTH"
		delete  <nondigit> from global currLocVal
		put global currLocVal into universal currentWeek
		log universal currentWeek
		#Comparing the Current and Total work weeks
		if (universal currentWeek=universal currentWeek+11)
			LogWarning"The KR Summary Shows only 12 Weeks in the UI"
		else
			"SC2_CommonActions/sc2CommonHandlers".krSummaryAdjvalueVerification universal data.partNumber,universal currentWeek,universal currentWeek,universal data.categoryInForecast, universal updatedTotalValue
		end if
		----------------Step-4------------------
		#Navigate to the  Planner Horizontal View
		"SC2_CommonActions/sc2CommonHandlers".navigateToPlannerHorizontalView
		smartUtil.completeScreenLoad
		"SC2_CommonActions/sc2CommonHandlers".navigateToSupplierName  universal data.supplierName
		smartUtil.completeScreenLoad
		#Searching for the Part Number
		smartUtil.clickText"Search","LHLH"
		smartUtil.textField universal data.partNumber
		log  universal data.partNumber
		smartUtil.clickUsingImage"icons/ForeCastAndCommitSearchIcon","LHLH"
		wait 5
		#Reading the KR value
		smartUtil.validate"KR","LHLH"
		smartUtil.clickTextAtSpot"KR",0,60,"LHLH"
		Typetext controlKey ,a,c
		put RemoteClipboard() into universal currentAdjvalue
		log  universal currentAdjvalue
		put universal currentAdjvalue+universal data.adjustmentPositiveValue into universal adjvalue
		smartUtil.textField  universal adjvalue
		smartUtil.clickText"Forecast Phase","topRightQuadrant"
		if ImageFound(text:"Adj Reason",searchRectangle:(screenPartition().SUT.("HM")),waitFor:2)
			smartUtil.setCustomSearchRectangle"Adj Reason",-80,0,150,90
			smartUtil.clickUsingImage "icons/adjReasonInputDropdown","Custompart"
			smartUtil.clickText universal data.EscalationReason
			smartUtil.clickUsingImage "icons/saveIconInForecastAndCommit","THTH"
			smartUtil.validate"Save Successfully","THTH"
		else
			smartUtil.clickUsingImage "icons/saveIconInForecastAndCommit","THTH"
			smartUtil.validate"Save Successfully","THTH"
		end if
		#Reading the Current Week
		smartUtil.readTextUsingText"Week",59,-10,38,15,"THTH"
		delete  <nondigit> from global currLocVal
		put global currLocVal into universal currentWeek
		log universal currentWeek
		if (universal currentWeek= universal currentWeek+11)
			LogWarning"The KR Summary Shows only 12 Weeks in the UI"
		else
			"SC2_CommonActions/sc2CommonHandlers".krSummaryAdjvalueVerification universal data.partNumber,universal currentWeek,universal currentWeek,universal data.categoryInForecast,universal adjvalue
			wait 3
		end if
		------------------Step-5-----------------------
		#Navigate toPlanner Horizonta lView
		"SC2_CommonActions/sc2CommonHandlers".navigateToPlannerHorizontalView
		smartUtil.completeScreenLoad
		"SC2_CommonActions/sc2CommonHandlers".navigateToSupplierName  universal data.supplierName
		smartUtil.completeScreenLoad
		#Searching for the Part Number
		smartUtil.clickText"Search","LHLH"
		smartUtil.textField universal data.partNumber
		log  universal data.partNumber
		smartUtil.clickUsingImage"icons/ForeCastAndCommitSearchIcon","LHLH"
		wait 5
		smartUtil.validate"KR","LHLH"
		smartUtil.clickTextAtSpot"KR",0,60,"LHLH"
		#Entering the negativeAdj value 
		smartUtil.textField  universal data.adjNV
		smartUtil.clickText"Forecast Phase","topRightQuadrant"
		smartUtil.validate"Cannot input negative KR quantity","THTH"
		------------------Step-6--------------------------
		#Validating the  TAA & NON-TAA  toggle
		smartUtil.clickText"View TAA & NON-TAA","THTH"
		smartUtil.completeScreenLoad
		wait 10
		set the TextStyle to default
		smartUtil.setCustomSearchRectangle"Demand Category",-30,10,100,150,"HM"
		if ImageFound(text:"SYSTEM/HARDBOOK",searchRectangle:(screenPartition().SUT.("Custompart")),waitFor:2) or  ImageFound(text:"FORECAST",searchRectangle:(screenPartition().SUT.("Custompart")),waitFor:2)
			LogError "The View TAA & NON-TAA toggle is  not Turned  On"
		else
			smartUtil.validate"NON-TAA","Custompart"
			smartUtil.validate"TAA","Custompart"
			LogSuccess "The View TAA & NON-TAA toggle is  Turned  On"
		end if
		smartUtil.clickText"View TAA & NON-TAA","THTH"
		---------------Step-7-----------------------
		#validating the  SYSTEM/HARDBOOK toggle
		smartUtil.clickText"SYSTEM/HARDBOOK","THTH"
		smartUtil.completeScreenLoad
		###The Screen is taking time to load so giving Waits to Load the Screen
		wait 5
		smartUtil.setCustomSearchRectangle"Demand Category",-30,10,100,300
		
		if ImageFound(text:"SYSTEM/HARDBOOK",searchRectangle:(screenPartition().SUT.("Custompart")),waitFor:2)
			LogError "The SYSTEM/HARDBOOK toggle is  not Turned  On"
		else
			smartUtil.validate"NON-TAA","Custompart"
			smartUtil.validate"TAA","Custompart"
			smartUtil.validate"FORECAST","Custompart"
			LogSuccess "The SYSTEM/HARDBOOK toggle is  Turned  On"
		end if
	Catch theException
		smartUtil.errorCapture theException
	End try	
end UIValidationForAdjPlannerHorizontalViewPart1


to UIValidationForAdjPlannerHorizontalViewPart2 supplierName
	try
		-------------Step-1----
		if (universal data.supplierName is "Jabil") then 
			put universal data.BJOriPartnumber into universal data.blankOriPartNumber
			put universal data.JBlankOriWW into  universal data.blankOriWW
		end if
		if (universal data.supplierName is "Venture") then 
			put universal data.BVOriPartnumber into universal data.blankOriPartNumber
			put universal data.VBlankOriWW into  universal data.blankOriWW
		end if
		if (universal data.supplierName is "Celestica") then 
			put universal data.BCOriPartnumber into universal data.blankOriPartNumber
			put universal data.CBlankOriWW into  universal data.blankOriWW
		end if
		log"Input KR in the WW not having any demand (ori = blank)"
		#navigating to the Planner Horizontal View
		"SC2_CommonActions/sc2CommonHandlers".navigateToPlannerHorizontalView
		#Waiting for the Complete Screen to Load
		smartUtil.completeScreenLoad
		#Clicking on the Supplier Name
		"SC2_CommonActions/sc2CommonHandlers".navigateToSupplierName universal data.supplierName
		smartUtil.completeScreenLoad
		wait 5
		#Reading the Current week
		smartUtil.readTextUsingText"Week",59,-10,38,15,"THTH"
		delete  <nondigit> from global currLocVal
		--put global currLocVa
		--log global currLocVal
		put global currLocVal into universal currentWeek
		log universal currentWeek
		put universal currentWeek+20 into universal totalWW
		log universal totalWW
		
		if universal totalWW>52
			put  universal totalWW-52 into universal totalWW
			log universal totalWW
		else 
			put   universal totalWW into  universal totalWW
			log  universal totalWW
		end if
		
		#Selecting the part Number and work week in the Filters
		wait 1
		smartUtil.clickText"Supplier","topLeftQuadrant"
		smartUtil.validate"Filters","topLeftQuadrant"
		"SC2_CommonActions/sc2CommonHandlers".selectPreference"Part Number",universal data.blankOriPartNumber
		smartUtil.clickText"Part Number","LHLH"
		smartUtil.scrollTo"Submit","5"
		smartUtil.clickTextAtSpot "Week From",260,25,"LH"
		"SC2_CommonActions/sc2CommonHandlers".selectWeekInKRSummary universal currentWeek
		smartUtil.clickTextAtSpot "Week To",260,25,"LH"
		put universal currentWeek+22 into universal morethan20ww
		log universal morethan20ww
		if universal morethan20ww>52
			put universal morethan20ww-52-1 into universal wwinCalender
			log universal wwinCalender
		end if 
		
		put universal currentWeek+19 into universal ww20
		log universal ww20
		if universal ww20>52
			put universal ww20-52 into ww20inApplication
			log ww20inApplication
		else 
			put  universal ww20 into ww20inApplication
			log ww20inApplication
		end if
		"SC2_CommonActions/sc2CommonHandlers".selectWeekInKRSummary ww20inApplication
		smartUtil.clickText"Submit","LHLH"
		smartUtil.validate"Forecast phase","topRightQuadrant"
		#Searching for the Ori value is Blank in the UI
		wait 4
		smartUtil.setCustomSearchRectangle"Forecast phase",-1200,180,1000,230,"topRightQuadrant"
		set the TextStyle to {dpi:144,textDifference:1}
		if ImageFound(0,text:"Week"&&universal data.blankOriWW,searchRectangle:(screenPartition().SUT.("Custompart")),waitFor:2)
			smartUtil.validate universal data.blankOriWW,"Custompart"	
		else
			Repeat until ImageFound(text:"Week"&&universal data.blankOriWW,searchRectangle:(screenPartition().SUT.("Custompart")),waitFor:2)
				Repeat 5 times
					smartUtil.clickUsingImage "icons/rightScroll","bottomRightQuadrant"
				End Repeat
				if ImageFound(0,text:"TOTAL",searchRectangle:(screenPartition().SUT.("Custompart")),waitFor:1)
					LogWarning "the Given Work week is not showing in the UI "
					Exit repeat
				end if
			End repeat
			repeat 10 times
				smartUtil.clickUsingImage "icons/rightScroll","bottomRightQuadrant"
			end repeat
		end if
		set the TextStyle to default
		#Entering some data in the KR value for Ori value is Blank
		smartUtil.validate"Week"&&universal data.blankOriWW,"Custompart"
		smartUtil.setCustomSearchRectangle"Week"&&universal data.blankOriWW,-65,40,200,350,"HM"
		smartUtil.clickTextAtSpot"KR",0,50,"Custompart"
		smartUtil.textField universal data.adjustmentPositiveValue
		smartUtil.clickText"Forecast Phase","topRightQuadrant"
		if ImageFound(text:"Adj Reason",searchRectangle:(screenPartition().SUT.("HM")),waitFor:2)
			smartUtil.setCustomSearchRectangle"Adj Reason",-80,0,150,90
			smartUtil.clickUsingImage "icons/adjReasonInputDropdown","Custompart"
			wait 2
			set the TextStyle to {dpi:144,textDifference:1}
			smartUtil.clickText universal data.EscalationReason,"HM"
			set the TextStyle to default
			smartUtil.clickUsingImage "icons/saveIconInForecastAndCommit","THTH"
			smartUtil.validate"Save Successfully","THTH"
		else
			repeat until ImageFound(image: "icons/adjReasonInputDropdown",searchRectangle:(screenPartition().SUT.("HM")),waitFor:2)
				repeat 3 times
					smartUtil.clickUsingImage "icons/rightScroll","bottomRightQuadrant"
				end repeat
				if repeatindex()>50
					exit repeat
				end if
			end repeat
			smartUtil.setCustomSearchRectangle"Adj Reason",-80,0,150,90
			smartUtil.clickUsingImage "icons/adjReasonInputDropdown","Custompart"
			smartUtil.clickText universal data.EscalationReason,"HM"
			smartUtil.clickUsingImage "icons/saveIconInForecastAndCommit","THTH"
			smartUtil.validate"Save Successfully","THTH"
		end if	
		smartUtil.setCustomSearchRectangle"Week"&&universal data.blankOriWW,-70,40,20,350,"HM"
		set the TextStyle to {dpi:144,textDifference:1}
		smartUtil.readTextUsingText"Ori",-10,30,25,90,"Custompart"
		put  global currLocVal into oriValue
		if (oriValue=0)
			LogSuccess "Ori value Become 0 Successfully after editing the KR value for the Blank Ori value"
		else 
			LogError"Ori value is not Equals to 0 after editing the KR value for the Blank Ori value"
		end if
		smartUtil.readTextUsingText"Adj",-10,30,25,90,"Custompart"
		put global currLocVal into AdjValue
